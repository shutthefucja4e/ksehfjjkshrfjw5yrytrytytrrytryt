-- Library

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("üèÜ - Sisyphus Sim")

local b = w:CreateFolder("Main")
local c = w:CreateFolder("Credits")

-- Variables

local Players = game:GetService("Players")

local lp = Players.LocalPlayer

-- Main

b:Toggle("AutoFarmWins",function(bool)
    getgenv().AutoFarmWins = bool
    
    task.spawn(function()
        while task.wait() do
            if AutoFarmWins then
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerTryBall"):FireServer(5)
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerEnd"):FireServer(true, 1)
            end
        end
    end)
end)

b:Toggle("AutoFarmGiftsRage",function(bool)
    getgenv().AutoFarmGiftsRage = bool
    
    task.spawn(function()
        while task.wait() do
            if AutoFarmGiftsRage then
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerTryBall"):FireServer(-1)
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerEnd"):FireServer(false, 1)
                
            end
        end
    end)
end)

b:Toggle("AutoFarmGiftsLegit",function(bool)
    getgenv().AutoFarmGiftsLegit = bool
    
    task.spawn(function()
        while task.wait() do
            if AutoFarmGiftsLegit then
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerTryBall"):FireServer(-1)
                wait(0.1)
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerEnd"):FireServer(false, 0.999999999999999998)
                
            end
        end
    end)
end)

b:Toggle("AutoClicker",function(bool)
    getgenv().AutoClicker = bool
    
    task.spawn(function()
        while task.wait() do
            if AutoClicker then
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerClick"):FireServer(game:GetService("Players").LocalPlayer.Character:FindFirstChild("6_1"))
            end
        end
    end)
end)

b:Toggle("Good WalkSpeed",function(bool)
    getgenv().WalkSpeed = bool
    
    task.spawn(function()
        while task.wait() do
            if WalkSpeed then
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 80
            end
        end
    end)
end)

b:Toggle("AutoRebirth",function(bool)
    getgenv().AutoRebirth = bool
    
    task.spawn(function()
        while task.wait() do
            if AutoRebirth then
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Eco"):WaitForChild("[C-S]PlayerTryRebirth"):FireServer()
                task.wait(0.1)
            end
        end
    end)
end)

b:Toggle("AutoRelics",function(bool)
    getgenv().AutoRelics = bool
    
    task.spawn(function()
        while task.wait() do
            if AutoRebirth then
                game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Function"):WaitForChild("Ornaments"):WaitForChild("[C-S]PlayerTryDoLuck"):InvokeServer("5", 1)
                task.wait(5)
            end
        end
    end)
end)

warn("Ty for using my script bro <3 !")

b:DestroyGui()

c:Label("UI : Wally UI V3",{
    TextSize = 20; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
})

c:Label("Made by : trl#8281",{
    TextSize = 15; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
})

c:Label("Any Problems? ^ Add me :)",{
    TextSize = 15; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
